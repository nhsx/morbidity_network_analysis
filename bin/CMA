#!/usr/bin/env python3

""" Command line executable """

import sys
import CMA
import logging
from timeit import default_timer as timer

if __name__ == '__main__':
    args, function = CMA.parseArgs.parseArgs()
    start = timer()
    rc = function(**vars(args))
    end = timer()
    logging.info(f'Total execution time: {end - start:.3f} seconds.')
    logging.shutdown()
    sys.exit(rc)
